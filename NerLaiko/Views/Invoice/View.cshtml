@model NerLaiko.Models.Invoice

@{
    ViewBag.Title = "Invoice";
    Layout = "_Layout";
}

<h2>Invoice</h2>

@if (Model == null)
{
    <h3>Invalid invoice.</h3>
}
else
{
    <h3>@Model.CreationDate</h3>
    <div class="container">
        @Html.Raw(Model.HtmlContents)
    </div>
}

<a href="@Url.Action("Index")">Return to main page.</a>